{
  "name": "next-template",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf .next && rimraf build && rimraf out && rimraf node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postbuild": "next-sitemap",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "npm run clear && ANALYZE=true npm run build",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --color --cache --cache-location 'node_modules/.cache/.eslintcache' src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "typegen": "graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "dayjs": "1.10.7",
    "gsap": "file:gsap-bonus.tgz",
    "html-react-parser": "1.4.8",
    "lodash": "4.17.21",
    "next": "11.1.2",
    "next-seo": "4.28.1",
    "next-sitemap": "3.1.11",
    "react": "17.0.2",
    "react-animate-height": "2.0.23",
    "react-device-detect": "2.1.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-markdown": "7.0.1",
    "react-share": "4.4.0",
    "react-transition-group": "4.4.2",
    "react-use": "17.3.1",
    "sharp": "0.29.3",
    "styled-components": "5.3.0",
    "swiper": "6.5.6",
    "twin.macro": "2.4.1",
    "zustand": "3.5.12"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@next/bundle-analyzer": "10.2.0",
    "@svgr/webpack": "5.5.0",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@types/body-scroll-lock": "3.1.0",
    "@types/css-font-loading-module": "0.0.6",
    "@types/lodash": "4.14.175",
    "@types/node": "15.3.0",
    "@types/react": "17.0.6",
    "@types/react-transition-group": "4.4.3",
    "@types/styled-components": "5.1.9",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-twin": "1.0.2",
    "eslint": "7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "15.4.0",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.11.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "8.0.0",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "tailwindcss": "2.1.2",
    "typescript": "4.4.4"
  }
}
